nav#navigation
  ul
    li.latest: a(href="/") Visa senaste
    li.about:  a(href="/om") Om

  table
    each months, year in navigation
      tr
        th
          a(href="/#{year}")= year
        each month in months
          td
            if month.link
              a(href=month.path, title="#{month.long} #{year}")= month.short
            else
              span= month.short