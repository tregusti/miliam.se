doctype 5
html
  head
    title
      if title
        = title
        = " ● Miliam Jorde"
      else
        = "Miliam Jorde"
    link(rel='stylesheet', href='/app.css')
    link(rel='shortcut icon', type='image/x-icon', href='/favicon.ico')
    link(rel='icon', type='image/x-icon', href='/favicon.png')
    link(rel="alternate", type="application/rss+xml", title="Miliam Jorde", href="/rss.xml")
    meta(name="viewport", content="device-width,initial-scale=1")
    if analytics
      script(src="/analytics.js")

    meta(name="description", content=description)
    meta(property="og:title", content=title || "Miliam Jorde")

    //- FB stuff
    script(src="/fb.js")
    meta(property="fb:admins", content="623506486,591318200")
    meta(property="og:url", content="http://miliam.se" + path)

    //- gplus url
    link(rel="canonical", href="http://miliam.se" + path)

  body
    hgroup#head
      a(href="/")
        h1 Miliam Jorde
        h2
          = "är nu "
          span= humanAge
          = " gammal"
    #content
      block content

    footer#footer
      aside#about.
        Liam är son till Sigyn och Glenn Jorde

    //- Google plus stuff
    script(src="/plusone.js")